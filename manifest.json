{
  "manifest_version": 3,
  "name": "CRXJS React Vite Example",
  "version": "1.0.0",
  "action": { "default_popup": "index.html" },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content-isolated.ts"]
    }
  ],
  "externally_connectable": {
    "ids": [],
    "matches": ["<all_urls>"]
  }
}